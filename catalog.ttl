# Observable Properties (SOSA/SSN)
dcm:cpu-usage-user a sosa:ObservableProperty ;
  rdfs:label "CPU Usage - User Space" ;
  rdfs:comment "Percentage of CPU time spent executing user space processes" ;
  qudt:hasUnit <http://qudt.org/vocab/unit/PERCENT> .

dcm:cpu-usage-system a sosa:ObservableProperty ;
  rdfs:label "CPU Usage - System/Kernel" ;
  rdfs:comment "Percentage of CPU time spent in kernel mode" ;
  qudt:hasUnit <http://qudt.org/vocab/unit/PERCENT> .

dcm:cpu-usage-idle a sosa:ObservableProperty ;
  rdfs:label "CPU Usage - Idle" ;
  rdfs:comment "Percentage of CPU time in idle state" ;
  qudt:hasUnit <http://qudt.org/vocab/unit/PERCENT> .

dcm:cpu-usage-iowait a sosa:ObservableProperty ;
  rdfs:label "CPU Usage - I/O Wait" ;
  rdfs:comment "Percentage of CPU time waiting for I/O operations to complete" ;
  qudt:hasUnit <http://qudt.org/vocab/unit/PERCENT> .

dcm:memory-total a sosa:ObservableProperty ;
  rdfs:label "Total System Memory" ;
  rdfs:comment "Total physical memory capacity in bytes" ;
  qudt:hasUnit <http://qudt.org/vocab/unit/BYTE> .

dcm:memory-used a sosa:ObservableProperty ;
  rdfs:label "Used Memory" ;
  rdfs:comment "Amount of memory currently in use in bytes" ;
  qudt:hasUnit <http://qudt.org/vocab/unit/BYTE> .

dcm:memory-available a sosa:ObservableProperty ;
  rdfs:label "Available Memory" ;
  rdfs:comment "Amount of memory available for allocation in bytes" ;
  qudt:hasUnit <http://qudt.org/vocab/unit/BYTE> .

dcm:memory-used-percent a sosa:ObservableProperty ;
  rdfs:label "Memory Usage Percentage" ;
  rdfs:comment "Percentage of total memory in use" ;
  qudt:hasUnit <http://qudt.org/vocab/unit/PERCENT> .

dcm:disk-reads a sosa:ObservableProperty ;
  rdfs:label "Disk Read Operations" ;
  rdfs:comment "Number of completed read operations" ;
  qudt:hasUnit <http://qudt.org/vocab/unit/NUM> .

dcm:disk-writes a sosa:ObservableProperty ;
  rdfs:label "Disk Write Operations" ;
  rdfs:comment "Number of completed write operations" ;
  qudt:hasUnit <http://qudt.org/vocab/unit/NUM> .

dcm:disk-read-bytes a sosa:ObservableProperty ;
  rdfs:label "Disk Bytes Read" ;
  rdfs:comment "Total bytes read from disk" ;
  qudt:hasUnit <http://qudt.org/vocab/unit/BYTE> .

dcm:disk-write-bytes a sosa:ObservableProperty ;
  rdfs:label "Disk Bytes Written" ;
  rdfs:comment "Total bytes written to disk" ;
  qudt:hasUnit <http://qudt.org/vocab/unit/BYTE> .

dcm:network-bytes-recv a sosa:ObservableProperty ;
  rdfs:label "Network Bytes Received" ;
  rdfs:comment "Total bytes received through network interface" ;
  qudt:hasUnit <http://qudt.org/vocab/unit/BYTE> .

dcm:network-bytes-sent a sosa:ObservableProperty ;
  rdfs:label "Network Bytes Sent" ;
  rdfs:comment "Total bytes sent through network interface" ;
  qudt:hasUnit <http://qudt.org/vocab/unit/BYTE> .

dcm:network-packets-recv a sosa:ObservableProperty ;
  rdfs:label "Network Packets Received" ;
  rdfs:comment "Total packets received through network interface" ;
  qudt:hasUnit <http://qudt.org/vocab/unit/NUM> .

dcm:network-packets-sent a sosa:ObservableProperty ;
  rdfs:label "Network Packets Sent" ;
  rdfs:comment "Total packets sent through network interface" ;
  qudt:hasUnit <http://qudt.org/vocab/unit/NUM> .

dcm:ib-port-rcv-data a sosa:ObservableProperty ;
  rdfs:label "InfiniBand Port Receive Data" ;
  rdfs:comment "Total data received on InfiniBand port" ;
  qudt:hasUnit <http://qudt.org/vocab/unit/BYTE> .

dcm:ib-port-xmit-data a sosa:ObservableProperty ;
  rdfs:label "InfiniBand Port Transmit Data" ;
  rdfs:comment "Total data transmitted on InfiniBand port" ;
  qudt:hasUnit <http://qudt.org/vocab/unit/BYTE> .

dcm:cpu-freq-current a sosa:ObservableProperty ;
  rdfs:label "Current CPU Frequency" ;
  rdfs:comment "Current operating frequency of CPU" ;
  qudt:hasUnit <http://qudt.org/vocab/unit/HZ> .

dcm:cpu-freq-max a sosa:ObservableProperty ;
  rdfs:label "Maximum CPU Frequency" ;
  rdfs:comment "Maximum rated frequency of CPU" ;
  qudt:hasUnit <http://qudt.org/vocab/unit/HZ> .

dcm:cpu-throttle-count a sosa:ObservableProperty ;
  rdfs:label "CPU Throttle Count" ;
  rdfs:comment "Number of times CPU has been thermally throttled" ;
  qudt:hasUnit <http://qudt.org/vocab/unit/NUM> .

dcm:smart-health-ok a sosa:ObservableProperty ;
  rdfs:label "SMART Health Status" ;
  rdfs:comment "Overall health status of storage device (true=healthy, false=failing)" ;
  qudt:hasUnit <http://qudt.org/vocab/unit/UNITLESS> .

dcm:temperature-celsius a sosa:ObservableProperty ;
  rdfs:label "Temperature" ;
  rdfs:comment "Temperature measurement in degrees Celsius" ;
  qudt:hasUnit <http://qudt.org/vocab/unit/DEG_C> .

dcm:fan-speed-rpm a sosa:ObservableProperty ;
  rdfs:label "Fan Speed" ;
  rdfs:comment "Fan rotation speed in revolutions per minute" ;
  qudt:hasUnit <http://qudt.org/vocab/unit/REV-PER-MIN> .

dcm:smart-raw-value a sosa:ObservableProperty ;
  rdfs:label "SMART Raw Value" ;
  rdfs:comment "Raw value of SMART attribute" ;
  qudt:hasUnit <http://qudt.org/vocab/unit/UNITLESS> .

dcm:process-cpu-usage a sosa:ObservableProperty ;
  rdfs:label "Process CPU Usage" ;
  rdfs:comment "CPU usage percentage for specific process" ;
  qudt:hasUnit <http://qudt.org/vocab/unit/PERCENT> .

dcm:process-memory-rss a sosa:ObservableProperty ;
  rdfs:label "Process Resident Set Size" ;
  rdfs:comment "Resident set size (physical memory) used by process" ;
  qudt:hasUnit <http://qudt.org/vocab/unit/BYTE> .

# Data Collection Provenance (PROV-O)
dcm:data-collection-activity a prov:Activity ;
  prov:startedAtTime "2025-11-05T09:38:18Z"^^xsd:dateTime ;
  prov:endedAtTime "2025-11-06T11:03:17Z"^^xsd:dateTime ;
  prov:wasAssociatedWith dcm:telegraf-agent, dcm:questdb-system ;
  rdfs:label "Data Collection Activity - November 2025" ;
  rdfs:comment "Automated collection of monitoring metrics from HPC infrastructure using Telegraf agents writing to QuestDB time-series database" .

dcm:telegraf-agent a prov:Agent, prov:SoftwareAgent ;
  rdfs:label "Telegraf Monitoring Agent" ;
  dct:description "Open-source server agent for collecting and reporting metrics and events from systems and applications" ;
  foaf:homepage <https://www.influxdata.com/time-series-platform/telegraf/> .

dcm:questdb-system a prov:Agent, prov:SoftwareAgent ;
  rdfs:label "QuestDB Time-Series Database" ;
  dct:description "High-performance time-series database for real-time analytics at Area Science Park" ;
