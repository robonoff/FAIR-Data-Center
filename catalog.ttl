# This catalog imports the Data Center Monitoring Ontology
<http://areasciencepark.it/datacenter/catalog> 
  owl:imports <http://127.0.0.1:8000/ontology.ttl> .

# Catalog
dcm:catalog a dcat:Catalog ;
  dct:title "Area Science Park Data Center Monitoring Catalog" ;
  dct:description "A catalog of time-series monitoring datasets from HPC infrastructure at Area Science Park, Padriciano, Trieste" ;
  dct:publisher [ 
    a foaf:Organization ;
    foaf:name "Area Science Park"
  ] ;
  dct:issued "2025-11-18"^^xsd:date ;
  dcat:dataset dcm:monitoring-nov2025 .

# Main Dataset
dcm:monitoring-nov2025 a dcat:Dataset ;
  dct:identifier "70b45719-044f-44be-864a-a07ef53b13e0" ;
  dct:title "Data Center HPC Monitoring Dataset - November 2025" ;
  dct:description "Time-series monitoring data collected from HPC compute nodes at Area Science Park using Telegraf agents. Includes CPU metrics (usage, frequency, temperature), memory utilization, disk I/O operations, network traffic (Ethernet and InfiniBand), SMART disk health, IPMI hardware sensors, and process statistics. Data collected at 500ms-1s intervals and stored in QuestDB." ;
  dct:keyword "time series", "data center", "High Performance Computing", "monitoring", "sensors", "CPU", "memory", "disk", "network", "InfiniBand", "SMART", "IPMI", "Telegraf", "QuestDB" ;
  dct:issued "2025-11-18"^^xsd:date ;
  dct:modified "2025-11-18"^^xsd:date ;
  dct:creator [
    a foaf:Person ;
    foaf:name "Roberta Lamberti" ;
    foaf:mbox <mailto:roberta.lamberti@areasciencepark.it>
  ] ;
  dct:publisher [ 
    a foaf:Organization ;
    foaf:name "Area Science Park"
  ] ;
  dcat:contactPoint [
    a vcard:Individual ;
    vcard:fn "Roberta Lamberti" ;
    vcard:hasEmail <mailto:roberta.lamberti@areasciencepark.it>
  ] ;
  dct:license <https://creativecommons.org/licenses/by-nc-sa/4.0/> ;
  dct:temporal [
    a dct:PeriodOfTime ;
    dcat:startDate "2025-11-05T09:38:18Z"^^xsd:dateTime ;
    dcat:endDate "2025-11-06T11:03:17Z"^^xsd:dateTime
  ] ;
  dct:spatial [
    a dct:Location ;
    rdfs:label "Area Science Park, Padriciano, Trieste, Italy"
  ] ;
  dcat:distribution dcm:http-access ;
  prov:hadMember dcm:cpu-file, dcm:mem-file, dcm:diskio-file, dcm:net-file, 
    dcm:infiniband-file, dcm:linux-cpu-file, dcm:smart-device-file, 
    dcm:smart-attribute-file, dcm:ipmi-sensor-file, dcm:procstat-file, 
    dcm:procstat-lookup-file, dcm:turbostat-file .

# HTTP Distribution
dcm:http-access a dcat:Distribution ;
  dct:title "HTTP Online Access" ;
  dcat:accessURL <https://timeseriesdb.dev.rd.areasciencepark.it/> ;
  dcat:mediaType "text/csv" .

# CPU Metrics File
dcm:cpu-file a prov:Entity ;
  dct:title "cpu.csv" ;
  dcat:accessURL <http://areasciencepark.it/datacenter/datasets/monitoring-nov2025/files/cpu.csv> ;
  dct:format "text/csv" ;
  dct:conformsTo dcm:CPUMetricsCollection ;
  dct:description "CPU usage metrics collected at 500ms intervals. Includes per-core and total CPU usage (user, system, idle, iowait, irq, softirq, steal, guest). Conforms to SOSA/SSN sensor observation model." ;
  sosa:observedProperty dcm:cpu-usage-user, dcm:cpu-usage-system, dcm:cpu-usage-idle, dcm:cpu-usage-iowait ;
  prov:wasGeneratedBy dcm:data-collection-activity ;
  prov:wasDerivedFrom dcm:questdb-system .

